<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <% styleSheets.forEach(styleSheet => { %>
      <link rel="stylesheet" href="<%= styleSheet %>" />
    <% }) %>
    <link rel="stylesheet" href="<%= cdnPrefix + page %>.css" />
</head>
<body>
    <div id="root" style="width: 100%; height: 100%;"></div>
    <% scripts.forEach(script => { %>
        <link rel="stylesheet" href="<%= script %>" />
    <% }) %>
    <script type="text/javascript" src="<%= cdnPrefix %>vendor.js"></script>
    <script type="text/javascript" src="<%= cdnPrefix + page %>.js"></script>
</body>
</html>